# Example secrets.toml file for local development
# Copy this file and add your actual credentials

# Google Service Account JSON (as a JSON string or base64 encoded)
# GOOGLE_SERVICE_ACCOUNT_JSON = """{...}"""

# Google Sheet ID
# GOOGLE_SHEET_ID = "your-sheet-id"

# Google Sheet GID (optional, for specific worksheet)
# GOOGLE_SHEET_GID = "worksheet-gid"

# Published CSV URL (alternative to service account)
# GOOGLE_SHEET_CSV_URL = "https://docs.google.com/spreadsheets/d/.../export?format=csv"
